{
  "name": "@jessestricker/extra-projen",
  "description": "More projects and components for projen.",
  "repository": {
    "type": "git",
    "url": "https://github.com/jessestricker/extra-projen.git"
  },
  "scripts": {
    "build": "pnpx projen build",
    "bump": "pnpx projen bump",
    "clobber": "pnpx projen clobber",
    "compat": "pnpx projen compat",
    "compile": "pnpx projen compile",
    "default": "pnpx projen default",
    "eject": "pnpx projen eject",
    "eslint": "pnpx projen eslint",
    "package": "pnpx projen package",
    "package-all": "pnpx projen package-all",
    "package:js": "pnpx projen package:js",
    "post-compile": "pnpx projen post-compile",
    "post-upgrade": "pnpx projen post-upgrade",
    "pre-compile": "pnpx projen pre-compile",
    "prettier": "pnpx projen prettier",
    "release": "pnpx projen release",
    "test": "pnpx projen test",
    "test:watch": "pnpx projen test:watch",
    "unbump": "pnpx projen unbump",
    "upgrade": "pnpx projen upgrade",
    "watch": "pnpx projen watch",
    "projen": "pnpx projen"
  },
  "author": {
    "name": "Jesse Stricker",
    "email": "git@jessestricker.de",
    "organization": false
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "commit-and-tag-version": "^12",
    "constructs": "10.0.0",
    "eslint": "^9",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "jest-junit": "^16",
    "jsii": "~5.9.0",
    "jsii-diff": "^1.117.0",
    "jsii-pacmak": "^1.117.0",
    "jsii-rosetta": "~5.9.0",
    "prettier": "^3.6.2",
    "projen": "0.98.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "constructs": "^10",
    "projen": ">=0.98.1 <1.0.0"
  },
  "pnpm": {},
  "keywords": [
    "projen"
  ],
  "main": "lib/index.js",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.0",
  "jest": {
    "coverageProvider": "v8",
    "testMatch": [
      "<rootDir>/@(src|test)/**/*(*.)@(spec|test).ts?(x)",
      "<rootDir>/@(src|test)/**/__tests__/**/*.ts?(x)",
      "<rootDir>/@(projenrc)/**/*(*.)@(spec|test).ts?(x)",
      "<rootDir>/@(projenrc)/**/__tests__/**/*.ts?(x)"
    ],
    "clearMocks": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "cobertura",
      "text"
    ],
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-reports"
        }
      ]
    ],
    "transform": {
      "^.+\\.[t]sx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.dev.json"
        }
      ]
    }
  },
  "types": "lib/index.d.ts",
  "stability": "stable",
  "jsii": {
    "outdir": "dist",
    "targets": {},
    "tsc": {
      "outDir": "lib",
      "rootDir": "src"
    }
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.ts and run \"pnpx projen\"."
}
